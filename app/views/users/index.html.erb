<table>
  <thead>
    <tr>
      <th>Profile_image</th>
      <th>Name</th>
    </tr>
  </thead>
<tbody>
<% @users.each do |user| %>
        <tr>
          <td>
          <%= link_to user_path(user.id) do %>
          <%= image_tag user.get_profile_image(100,100) %>
          <% end %>
         </td>
         <td><%= user.name %></td>
        </tr>
        <% end %>

</tbody>
</table>